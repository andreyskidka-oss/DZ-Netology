def find_max_sales_channel(stats):
    max_channel = None
    max_sales = 0
    
    for channel, sales in stats.items():
        if sales > max_sales:
            max_sales = sales
            max_channel = channel
    
    return max_channel

# Пример использования:
stats = {'facebook': 55, 'yandex': 115, 'vk': 120, 'google': 99, 'email': 42, 'ok': 98}

max_channel = find_max_sales_channel(stats)
print(f"Максимальный объем продаж на рекламном канале: {max_channel}")

# Альтернативный способ с использованием max()
max_channel_alt = max(stats, key=stats.get)
print(f"Проверка (способ 2): {max_channel_alt}")
